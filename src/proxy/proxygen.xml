	<template src="proxy/backFoundation.png" default="True">
		<matches>
			<match name="Type" value="" />
		</matches>
		<overlayblocks>
			<conditional>
				<switch property="Type">
					<case value="Foundation">
						<link block="backFoundation" />
						<link block="difficultyFoundation" />
						<link block="controlFoundation" />
					</case>
					<case value="Asset">
						<link block="backAsset" />
						<link block="difficultyAsset" />
						<link block="controlAsset" />
					</case>
					<case value="Action">
						<link block="backAction" />
						<link block="difficultyAction" />
						<link block="controlAction" />
					</case>
					<case value="Char">
						<link block="backChar" />
						<link block="difficultyChar" />
						<link block="controlChar" />
					</case>
					<case value="Attack">
						<link block="backAttack" />
						<link block="difficultyAttack" />
						<link block="controlAttack" />
					</case>
				</switch>
			</conditional>
			<conditional>
				<if property="Block Zone" value="High">
					<link block="blockHigh" />
				</if>
				<elseif property="Block Zone" value="Mid">
					<link block="blockMid" />
				</elseif>
				<elseif property="Block Zone" value="Low">
					<link block="blockLow" />
				</elseif>
			</conditional>
			<conditional>
				<if property="Attack Zone" value="High">
					<link block="speedHigh" />
				</if>
				<elseif property="Attack Zone" value="Mid">
					<link block="speedMid" />
				</elseif>
				<elseif property="Attack Zone" value="Low">
					<link block="speedLow" />
				</elseif>
			</conditional>
			<conditional>
				<if property="Split Difficulty" value="#NULL#" >
					<link block="blank" />
				</if>
				<else>
					<link block="difficultySplit" />
				</else>
			</conditional>
			<conditional>
				<if property="Hand Size" value="#NULL#" >
					<link block="blank" />
				</if>
				<else>
					<link block="hand" />
				</else>
			</conditional>
			<conditional>
				<if property="Vitality" value="#NULL#" >
					<link block="blank" />
				</if>
				<else>
					<link block="vit" />
				</else>
			</conditional>
			<conditional>
				<if property="Damage" value="#NULL#" >
					<link block="blank" />
				</if>
				<else>
					<link block="damage" />
				</else>
			</conditional>
		</overlayblocks>
		<textblocks>
			<link block="nameText">
				<property name="Name" />
			</link>
			<link block="splitDifficultyText">
				<property name="Split Difficulty" />
			</link>
			<link block="difficultyText">
				<property name="Difficulty" />
			</link>
			<link block="blockText">
				<property name="Block Modifier" />
			</link>
			<link block="resText">
				<property name="Resources" />
			</link>
			<link block="splitEffectText" separator="&#10;" >
				<property name="Split Keywords" />
				<property name="Split Rules" />
			</link>
			<link block="effectText">
				<property name="CardText" />
			</link>
			<link block="speedText">
				<property name="Speed" />
			</link>
			<link block="damageText">
				<property name="Damage" />
			</link>
			<link block="handText">
				<property name="Hand Size" />
			</link>
			<link block="vitText">
				<property name="Vitality" />
			</link>
			<link block="controlText">
				<property name="Control" />
			</link>
			<link block="keywordText">
				<property name="Keywords" />
			</link>
			<link block="rarityText">
				<property name="Rarity" />
			</link>
		</textblocks>
	</template>
</templates>
